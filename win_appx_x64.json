{
    "appId": "cn.cnoctave.marked_reader",
    "productName": "marked_reader",
    "buildNumber": "2.1.0",
    "icon": "logo.png",
    "directories": {
        "output": "dist"
    },
    "publish": [
        {
        "provider": "github",
        "owner": "CNOCTAVE",
        "repo": "marked_reader",
        "releaseType": "release"
        }
    ],
    "win": {
        "artifactName": "${name}-${version}-${os}-${arch}-appx.${ext}",
        "target": [
            {
                "target": "appx",
                "arch": [
                    "x64"
                ]
            }
        ]
    },
    "appx": {
        "applicationId": "marked.reader",
        "identityName": "marked.reader",
        "displayName": "Marked Reader",
        "languages": ["zh-cn"],
        "backgroundColor": "transparent",
        "publisherDisplayName": "***********"
    }
}